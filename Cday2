    printf("%d", ch);
    return 0;
}
#include <stdio.h>
int main(){
    int x;
    printf("Write a number:");
    scanf("%d", &x);

    if (x%5==0){
        printf("Divisible by 5");
    }
    else{
        printf("Not divisible by 5");
    }
    return 0;
}
